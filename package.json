{
  "name": "Ah-frontend-guardians",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --port 4000 --config ./webpack.config.js --mode development --hot --open --https",
    "build": "webpack --mode production",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "cypress:open": "cypress open",
    "heroku-postbuild": "npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/src/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/setupTests.js",
      "<rootDir>/cypress"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/src/store/combineStore.js",
      "<rootDir>/src/index.js",
      "<rootDir>/src/reducers/mainReducer.js",
      "<rootDir>/src/routes/index.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/assetsTransformer.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/setupTests.js",
      "!src/index.js",
      "!**/node_modules/**"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela/Ah-frontend-guardians.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela/Ah-frontend-guardians/issues"
  },
  "homepage": "https://github.com/andela/Ah-frontend-guardians#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "axios-mock-adapter": "^1.16.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.1.0",
    "jest-mock-axios": "^2.1.11",
    "jsonwebtoken": "^8.4.0",
    "moxios": "^0.4.0",
    "node-sass": "^4.11.0",
    "react-hot-loader": "^4.6.5",
    "react-social-icons": "^4.1.0",
    "react-test-renderer": "^16.8.2",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "scss-loader": "0.0.1",
    "sinon": "^7.2.4",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^4.3.1",
    "cypress": "^3.1.5",
    "express": "^4.16.4",
    "express-yields": "^1.1.2",
    "fs-extra": "^7.0.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "image-loader": "0.0.1",
    "loader": "^2.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.1",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-dom": "^16.8.1",
    "react-quill": "^1.3.3",
    "react-facebook-login": "^4.1.1",
    "react-google-login": "^5.0.2",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-tagsinput": "^3.19.0",
    "react-toastify": "^4.5.2",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "sinon": "^7.2.4",
    "style": "0.0.3",
    "style-loader": "^0.23.1",
    "sweetalert": "^2.1.2",
    "webpack-dev-middleware": "^3.5.2",
    "webpack-hot-middleware": "^2.24.3"
  }
}
